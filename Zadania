        # Funkcja pobierająca listę liczb i zwracającą drugą najmniejszą wartość (wszystkie wystąpienia) #

def zlicz_druga_najmniejsza(tablica):
    tablica.sort()
    count = tablica.count(tablica[1])
    for i in range(0, count):
        print(tablica[1])


tablica = [1, 67, 2, 4, 5, 5, 77, 2, 3, 6, 8, 3]
print(zlicz_druga_najmniejsza(tablica))


        # Program wypisujący następne 20 lat przestępnych #



print("Witaj w kalkulatorze roku przestępnego na kolejne 20lat")
print("Wpisz aktualny rok: ")
rok = input()
print("Wybrany przez Ciebie rok to: " + rok)
print("Poniżej znajduje się lista kolejnych roków przestępnych na okres 20lat")

def przestepny(rok):
    for i in range((int(rok)), (int(rok)) + 21):
        if i % 4 == 0 and i % 100 != 0:
            print(i)

print(przestepny(rok))





        # Program wypisujący tabliczkę mnożenia od 1 do 12 #



def tabliczka_mnozenia(range):
    for a in range (1,13):
        for b in range (1,13):
            print(a, "*", b, "=", a*b)

print(tabliczka_mnozenia(range))



